{
  "name": "@won-storage/react",
  "version": "1.0.1",
  "description": "React hooks library for storage management using useSyncExternalStore",
  "author": "wondonghwi",
  "license": "MIT",
  "keywords": [
    "react",
    "hooks",
    "storage",
    "localStorage",
    "sessionStorage",
    "useSyncExternalStore"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wondonghwi/won-storage.git",
    "directory": "packages/react"
  },
  "bugs": {
    "url": "https://github.com/wondonghwi/won-storage/issues"
  },
  "homepage": "https://github.com/wondonghwi/won-storage#readme",
  "type": "module",
  "private": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "dependencies": {
    "@won-storage/core": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^19.1.16",
    "react": "^19.1.1",
    "typescript": "~5.9.3",
    "vite": "npm:rolldown-vite@7.1.14",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20.19.0"
  }
}
