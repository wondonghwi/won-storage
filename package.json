{
  "name": "won-storage-monorepo",
  "version": "0.0.0",
  "description": "Storage management library with framework-agnostic core and React bindings",
  "author": "wondonghwi",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm --filter won-storage dev",
    "build": "pnpm --filter @won-storage/core build && pnpm --filter won-storage build",
    "build:core": "pnpm --filter @won-storage/core build",
    "build:react": "pnpm --filter won-storage build",
    "lint": "pnpm --recursive run lint",
    "prepare": "husky",
    "format": "prettier \"**/*.{ts,tsx,js,jsx,json,css,md}\" --write",
    "format:check": "prettier \"**/*.{ts,tsx,js,jsx,json,css,md}\" --check",
    "test": "pnpm --recursive run test",
    "test:watch": "pnpm --recursive run test:watch",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm build && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@eslint/js": "^9.36.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "husky": "^9.1.6",
    "jsdom": "^27.0.0",
    "prettier": "^3.4.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "npm:rolldown-vite@7.1.14",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.1.14"
    }
  }
}
